{
    "name": "trustchain-monorepo",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@4.1.1",
    "workspaces": [
        "frontend",
        "backend"
    ],
    "scripts": {
        "start": "yarn workspaces run start",
        "start:frontend": "yarn workspace trustchain-frontend start",
        "start:backend": "yarn workspace trustchain-backend start",
        "build": "yarn workspaces run build",
        "test": "yarn workspaces run test"
    }
}