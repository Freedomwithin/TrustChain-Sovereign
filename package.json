{
    "name": "trustchain-monorepo",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@4.7.0",
    "workspaces": [
        "backend",
        "frontend"
    ],
    "scripts": {
        "setup:full": "./setup_full.sh",
        "build:frontend": "yarn workspace trustchain-vite build",
        "build:backend": "yarn workspace trustchain-backend build",
        "dev": "yarn workspaces foreach -Api run dev"
    },
    "devDependencies": {
        "webpack-bundle-analyzer": "^4.10.2"
    },
    "dependencies": {
        "uuid": "^13.0.0"
    }
}
